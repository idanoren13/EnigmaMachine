<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontEnd.controllers.LoginController">
    <content>
        <GridPane hgap="3.0" minHeight="500.0" minWidth="900" prefHeight="500.0" prefWidth="900.0" vgap="3.0">
            <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="NEVER" maxWidth="214.0" minWidth="49.0" prefWidth="60.0" />
                <ColumnConstraints halignment="RIGHT" hgrow="NEVER" maxWidth="655.0" minWidth="49.0" prefWidth="198.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="840.0" minWidth="67.0" prefWidth="364.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="840.0" minWidth="71.0" prefWidth="457.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints vgrow="NEVER" />
                <RowConstraints maxHeight="281.0" minHeight="40.0" prefHeight="98.0" vgrow="NEVER" />
                <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="50.0" vgrow="ALWAYS" />
                <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="62.0" vgrow="ALWAYS" />
                <RowConstraints maxHeight="1.7976931348623157E308" minHeight="0.0" prefHeight="186.0" vgrow="ALWAYS" />
                <RowConstraints maxHeight="251.0" minHeight="70.0" prefHeight="85.0" vgrow="NEVER" />
            </rowConstraints>
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="User Name:" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                <TextField fx:id="userNameTextField" onKeyTyped="#userNameKeyTyped" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <HBox alignment="CENTER" prefHeight="103.0" prefWidth="488.0" spacing="15.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="5">
                    <children>
                        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
                        <Button id="quit-button" mnemonicParsing="false" onAction="#quitButtonClicked" text="Quit" />
                    </children>
                </HBox>
                <Label fx:id="errorMessageLabel" alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="173.0" prefWidth="302.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Threads number:" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Missions Size:" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <TextField fx:id="missionSizeCol" prefHeight="31.0" prefWidth="440.0" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Allies" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                <TableView fx:id="alliesTable" onMouseClicked="#selectAllyTeam" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="4">
                    <columns>
                        <TableColumn fx:id="allyCol" prefWidth="192.0" text="Ally" />
                        <TableColumn fx:id="isAvialableCol" prefWidth="202.0" text="isAvailable" />
                    </columns>
                </TableView>
                <ChoiceBox fx:id="threadsNumber" prefHeight="31.0" prefWidth="227.0" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </GridPane>
    </content>
</ScrollPane>
