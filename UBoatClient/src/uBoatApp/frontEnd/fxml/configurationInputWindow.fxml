<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.ListSelectionView?>


<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uBoatApp.frontEnd.ConfigurationInputWindowController">
   <children>
      <Label alignment="CENTER" prefWidth="436.0" text="Select Rotors">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ListSelectionView fx:id="rotorIDs" />
      <Label alignment="CENTER" prefWidth="436.0" text="Select a Reflector">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ListSelectionView fx:id="reflectorIDs" />
   </children>
</VBox>
